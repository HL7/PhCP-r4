    <kind value="resource"/>
    <abstract value="false"/>
    <type value="ServiceRequest"/>
    <baseDefinition value="http://hl7.org/fhir/StructureDefinition/ServiceRequest"/>
    <derivation value="constraint"/>
    <differential>
        <element id="ServiceRequest.occurrence[x]">
